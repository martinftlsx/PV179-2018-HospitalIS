@model IEnumerable<HospitalIS.WebApplication.Models.Disease.DiseaseInfoModel>

@{
    ViewBag.Title = "FilteredDiseasesView";
}

<h2>Potential diseases</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
